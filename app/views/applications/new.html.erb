<%= form_tag "/applications/", method: :post do %>
<%= label_tag "Name" %>
<%= text_field_tag :applicant %>
<%= label_tag "Street Address" %>
<%= text_field_tag :address %>
<%= label_tag "City" %>
<%= text_field_tag :city %>
<%= label_tag "State" %>
<%= text_field_tag :state %>
<%= label_tag "Zip Code" %>
<%= text_field_tag :zip %>
<%= submit_tag "Submit" %>
<% end %>
